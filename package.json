{
  "name": "instaapi",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "tsc",
    "lint": "eslint . & echo 'lint complete'",
    "dev": "ts-node-dev --respawn src/index.ts --poll",
    "prod": "tsc && node ./app/index.js"
  },
  "dependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/node-fetch": "^2.5.8",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "node-fetch": "^2.6.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "got": "^11.3.0",
    "tape": "^4.7.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vodolazskikh/instaapi.git"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
